<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<!-- Background -->
	<Color x:Key="DefaultBackground">#202020</Color>
	<Color x:Key="SecondaryBackground">#323232</Color>
	<Color x:Key="TertiaryBackground">#272727</Color>
	<Color x:Key="QuartenaryBackground">#333333</Color>

	<!-- ControlFill -->
	<Color x:Key="DefaultControlFill">#2C2C2C</Color>
	<Color x:Key="SecondaryControlFill">#2A2A2A</Color>
	<Color x:Key="TertiaryControlFill">#303030</Color>
	<Color x:Key="DefaultSubControlFill">#454545</Color>
	<Color x:Key="SecondarySubControlFill">#505050</Color>
	<Color x:Key="TertiarySubControlFill">#575757</Color>
	<Color x:Key="DisabledControlFill">#505050</Color>

	<!--TextFill-->
	<Color x:Key="DefaultTextFill">#FFFFFF</Color>
	<Color x:Key="DefaultAccentTextFill">#1A1A1A</Color>
	<Color x:Key="SecondaryTextFill">#CCCCCC</Color>
	<Color x:Key="TertiaryTextFill">#969696</Color>
	<Color x:Key="DisabledTextFill">#717171</Color>
	
</ResourceDictionary>

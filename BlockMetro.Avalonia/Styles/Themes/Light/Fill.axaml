<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<!-- Background -->
	<Color x:Key="DefaultBackground">#F3F3F3</Color>
	<Color x:Key="SecondaryBackground">#FDFDFD</Color>
	<Color x:Key="TertiaryBackground">#F9F9F9</Color>
	<Color x:Key="QuartenaryBackground">#F8F8F8</Color>
	
	<!-- ControlFill -->
	<Color x:Key="DefaultControlFill">#FFFFFF</Color>
	<Color x:Key="SecondaryControlFill">#E5E5E5</Color>
	<Color x:Key="TertiaryControlFill">#DCDCDC</Color>
	<Color x:Key="DefaultSubControlFill">#BABABA</Color>
	<Color x:Key="SecondarySubControlFill">#CCCCCC</Color>
	<Color x:Key="TertiarySubControlFill">#D3D3D3</Color>
	<Color x:Key="DisabledControlFill">#C4C4C4</Color>
	
	<!--TextFill-->
	<Color x:Key="DefaultTextFill">#1A1A1A</Color>
	<Color x:Key="SecondaryTextFill">#5C5C5C</Color>
	<Color x:Key="TertiaryTextFill">#868686</Color>
	<Color x:Key="DisabledTextFill">#9B9B9B</Color>
	
</ResourceDictionary>
